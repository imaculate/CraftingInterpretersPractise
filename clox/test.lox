fun fib(n) {
    if (n < 2) return n;
    return fib(n-2) + fib(n-1);
}

var start = clock();
print fib(35);
print clock() - start;

point = (x, y) -> method ->
        match method:
            "x" -> x
            "y" -> y
            set("x", newx) -> x = newx
            set("y", newy) -> y = newy
            "add" -> (pt) -> point(x + pt.x, y + pt.y)
p = point(1, 2)
p.x = 5
print p.add(point(3, 4))